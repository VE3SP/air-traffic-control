  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
	
  # Pods for IBMFlightTracker
  pod 'CocoaMQTT'
  pod 'CocoaAsyncSocket'
  pod 'SwiftyJSON'
  
  target 'ARFlightTracker-iOS-SwiftTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ARFlightTracker-iOS-SwiftUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
